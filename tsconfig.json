{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2021",
    "module": "es2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
